package com.bae.test;

import static org.junit.Assert.assertEquals;

import org.junit.Test;


public class test extends unittest {
	@Test
	public void test1() {
	assertEquals(10, blackjack.twist(25));
	}
	
	@Test
	public void test2() {
	assertEquals(10, blackjack.twist(10));
	}
	
	@Test
	public void test3() {
	assertEquals(0, blackjack.twist(25));
	}
	
	@Test
	public void test4() {
	assertEquals(21, blackjack.twist(10));
	}
	
	@Test
	public void test5() {
	assertEquals(21, blackjack.twist(21));
	}
	
	@Test
	public void test6() {
	assertEquals(-1, blackjack.twist(31));
	}
	
	@Test
	public void test7() {
	assertEquals(-1, blackjack.twist(1));
	}
	
	@Test
	public void test8() {
	assertEquals(-1, blackjack.twist(10));
	}
	
	@Test
	public void test9() {
	assertEquals(-1, blackjack.twist(10));
	}
	@Test
	public void test10() {
	assertEquals(-1, blackjack.twist(31));
	}
	@Test
	public void test11() {
	assertEquals(10, blackjack.twist(30));
	}
	@Test
	public void test12() {
	assertEquals(-1, blackjack.twist(1));
	}
	
	@Test
	public void test13() {
	assertEquals(10, blackjack.twist(4));
	}
}